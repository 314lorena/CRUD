@model PruebaIndexModelView;

@{
    ViewData["Title"] = "COMPRA";
    ViewData["Title_"] = "lISTADO DE COMPRA";
}

  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous" />
     <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
     <link rel="stylesheet" href="~/css~/css/site.css" />
    <div  id="contenedor">

<table class="table table-striped">
    <h1 class="display-4">Listados de Compra</h1>
 
  <thead >
    <tr>
      <th >Docuemento usuario</th>
      <th >Nombre Usuario</th>
      <th >Dirreccion</th>
      <th >Telefono</th>
      <th >Acciones</th>
      </tr>
  </thead>
  <tbody>

       @{
            foreach(var item in Model.ListadoDeUsuarios)
      {
          <tr>
                <td>@item.Docuemento_usuario </td>
                <td>@item.NombreUsuario </td>
                <td>@item.Dirreccion</td>             
                <td>@item.Telefono</td>  
                
              <td>
               <button class="btn btn-success">
                    <i class="bi bi-pencil-square">editar</i>
               </button> 

               
        <button class="btn btn-primary">
            <i class="bi bi-trash">Eliminar</i>
        </button>
               </td>

           </tr>
            }
   }
  </tbody>
    
</table>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
     
         </div>

